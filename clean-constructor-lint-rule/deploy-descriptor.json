{
  "package": {
    "name": "clean-constructor-lint-rule",
    "repo": "android",
    "subject": "grigory-rylov",
    "desc": "Android lint rule for detecting non clean constructors.",
    "website_url": "https://github.com/Grigory-Rylov/clean-constructor-lint-rule",
    "issue_tracker_url": "https://github.com/Grigory-Rylov/clean-constructor-lint-rule/issues",
    "vcs_url": "https://github.com/Grigory-Rylov/clean-constructor-lint-rule.git",
    "github_use_tag_release_notes": true,
    "github_release_notes_file": "RELEASE.txt",
    "licenses": [
      "Apache-2.0"
    ],
    "labels": [
      "android",
      "lint",
      "clean code"
    ],
    "public_download_numbers": false,
    "public_stats": false
  },
  "version": {
    "name": "${project.version}",
    "vcs_tag": "${project.version}",
    "released": "${build.timestamp}"
  },
  "files": [
    {
      "includePattern": "target\/((\\w+)-(\\d+).(\\d+).(\\d+).jar)",
      "excludePattern": ".*/do-not-deploy/.*",
      "uploadPattern": "com/github/grishberg/clean-constructor-lint-rule/$3.$4.$5/$2-$3.$4.$5.jar"
    },
    {
      "includePattern": "target\/((\\w+)-(\\d+).(\\d+).(\\d+).pom)",
      "excludePattern": ".*/do-not-deploy/.*",
      "uploadPattern": "com/github/grishberg/clean-constructor-lint-rule/$3.$4.$5/$2-$3.$4.$5.pom"
    },
    {
      "includePattern": "target\/((\\w+)-(\\d+).(\\d+).(\\d+)(-(\\w+)){1}.jar)",
      "excludePattern": ".*/do-not-deploy/.*",
      "uploadPattern": "com/github/grishberg/clean-constructor-lint-rule/$3.$4.$5/$2-$3.$4.$5$6.jar"
    }
  ],
  "publish": true
}